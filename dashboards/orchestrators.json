{
  "dashboard": {
    "title": "Orchestrators Dashboard",
    "description": "Real-time monitoring of orchestrator performance, execution status, and health metrics",
    "version": "1.0",
    "refresh_interval": "30s",
    "time_range": "1h",
    "panels": [
      {
        "id": "orchestrator_execution_summary",
        "title": "Orchestrator Execution Summary",
        "type": "stat",
        "position": {"x": 0, "y": 0, "w": 12, "h": 4},
        "targets": [
          {
            "query": "sli_orchestrator_total_count",
            "legend": "Total Executions",
            "format": "number"
          },
          {
            "query": "sli_orchestrator_success_count",
            "legend": "Successful Executions",
            "format": "number"
          },
          {
            "query": "sli_orchestrator_error_count",
            "legend": "Failed Executions",
            "format": "number"
          },
          {
            "query": "sli_orchestrator_retry_count",
            "legend": "Retry Attempts",
            "format": "number"
          }
        ],
        "thresholds": [
          {"color": "green", "value": null},
          {"color": "yellow", "value": 10},
          {"color": "red", "value": 50}
        ]
      },
      {
        "id": "orchestrator_latency_trend",
        "title": "Orchestrator Latency Trend",
        "type": "timeseries",
        "position": {"x": 0, "y": 4, "w": 8, "h": 6},
        "targets": [
          {
            "query": "sli_orchestrator_latency_ms_p50",
            "legend": "P50 Latency",
            "format": "ms"
          },
          {
            "query": "sli_orchestrator_latency_ms_p95",
            "legend": "P95 Latency",
            "format": "ms"
          },
          {
            "query": "sli_orchestrator_latency_ms_p99",
            "legend": "P99 Latency",
            "format": "ms"
          }
        ],
        "thresholds": [
          {"color": "green", "value": null},
          {"color": "yellow", "value": 2000},
          {"color": "red", "value": 5000}
        ],
        "y_axis": {
          "label": "Latency (ms)",
          "min": 0
        }
      },
      {
        "id": "orchestrator_error_rate",
        "title": "Orchestrator Error Rate",
        "type": "timeseries",
        "position": {"x": 8, "y": 4, "w": 4, "h": 6},
        "targets": [
          {
            "query": "sli_orchestrator_error_rate",
            "legend": "Error Rate",
            "format": "percent"
          }
        ],
        "thresholds": [
          {"color": "green", "value": null},
          {"color": "yellow", "value": 1.0},
          {"color": "red", "value": 5.0}
        ],
        "y_axis": {
          "label": "Error Rate (%)",
          "min": 0,
          "max": 10
        }
      },
      {
        "id": "node_execution_status",
        "title": "Node Execution Status",
        "type": "table",
        "position": {"x": 0, "y": 10, "w": 12, "h": 6},
        "targets": [
          {
            "query": "sli_node_status",
            "columns": [
              {"text": "Node ID", "field": "node_id"},
              {"text": "Status", "field": "status"},
              {"text": "Duration (ms)", "field": "duration_ms"},
              {"text": "Retries", "field": "retry_count"},
              {"text": "Errors", "field": "error_count"},
              {"text": "Last Executed", "field": "last_executed"}
            ]
          }
        ],
        "transformations": [
          {
            "type": "organize",
            "options": {
              "excludeByName": {},
              "indexByName": {},
              "renameByName": {}
            }
          }
        ]
      },
      {
        "id": "dag_execution_flow",
        "title": "DAG Execution Flow",
        "type": "graph",
        "position": {"x": 0, "y": 16, "w": 8, "h": 8},
        "targets": [
          {
            "query": "sli_dag_nodes",
            "legend": "DAG Nodes",
            "format": "graph"
          }
        ],
        "options": {
          "showLegend": true,
          "showValues": false,
          "orientation": "horizontal"
        }
      },
      {
        "id": "anomaly_scores",
        "title": "Anomaly Detection Scores",
        "type": "timeseries",
        "position": {"x": 8, "y": 16, "w": 4, "h": 8},
        "targets": [
          {
            "query": "sli_anomaly_score",
            "legend": "Anomaly Score",
            "format": "number"
          }
        ],
        "thresholds": [
          {"color": "green", "value": null},
          {"color": "yellow", "value": 0.7},
          {"color": "red", "value": 0.9}
        ],
        "y_axis": {
          "label": "Anomaly Score",
          "min": 0,
          "max": 1
        }
      },
      {
        "id": "resource_utilization",
        "title": "Resource Utilization",
        "type": "timeseries",
        "position": {"x": 0, "y": 24, "w": 6, "h": 6},
        "targets": [
          {
            "query": "sli_cpu_usage",
            "legend": "CPU Usage",
            "format": "percent"
          },
          {
            "query": "sli_memory_usage",
            "legend": "Memory Usage",
            "format": "percent"
          }
        ],
        "thresholds": [
          {"color": "green", "value": null},
          {"color": "yellow", "value": 70},
          {"color": "red", "value": 90}
        ],
        "y_axis": {
          "label": "Usage (%)",
          "min": 0,
          "max": 100
        }
      },
      {
        "id": "throughput_metrics",
        "title": "Throughput Metrics",
        "type": "timeseries",
        "position": {"x": 6, "y": 24, "w": 6, "h": 6},
        "targets": [
          {
            "query": "sli_requests_per_second",
            "legend": "Requests/sec",
            "format": "number"
          },
          {
            "query": "sli_concurrent_executions",
            "legend": "Concurrent Executions",
            "format": "number"
          }
        ],
        "y_axis": {
          "label": "Count",
          "min": 0
        }
      }
    ],
    "templating": {
      "list": [
        {
          "name": "orchestrator",
          "type": "query",
          "query": "sli_orchestrator_names",
          "refresh": 2,
          "includeAll": true,
          "multi": true
        },
        {
          "name": "org_id",
          "type": "query",
          "query": "sli_org_ids",
          "refresh": 2,
          "includeAll": true,
          "multi": true
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "annotations",
          "iconColor": "rgba(255, 96, 96, 1)",
          "enable": true
        },
        {
          "name": "Alerts",
          "datasource": "annotations",
          "iconColor": "rgba(255, 96, 96, 1)",
          "enable": true
        }
      ]
    },
    "links": [
      {
        "title": "Runbooks",
        "url": "/docs/runbooks/",
        "targetBlank": true
      },
      {
        "title": "SLO Dashboard",
        "url": "/dashboards/slo_board.json",
        "targetBlank": false
      },
      {
        "title": "Logs",
        "url": "/logs/orchestrators",
        "targetBlank": true
      }
    ]
  }
}
