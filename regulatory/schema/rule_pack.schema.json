{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://example.com/regulatory/rule_pack.schema.json",
  "title": "Regulatory Rule Pack",
  "type": "object",
  "additionalProperties": false,
  "required": ["versions"],
  "properties": {
    "versions": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "additionalProperties": false,
        "required": ["effective_from", "data"],
        "properties": {
          "effective_from": {
            "type": "string",
            "pattern": "^\\d{4}-\\d{2}-\\d{2}$"
          },
          "effective_to": {
            "type": ["string", "null"],
            "pattern": "^\\d{4}-\\d{2}-\\d{2}$"
          },
          "data": {
            "type": "object",
            "additionalProperties": true
          }
        }
      }
    }
  }
}


